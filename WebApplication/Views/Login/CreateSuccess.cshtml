            </dt>
            <dd class = "col-sm-10">
                @Html.DisplayFor(model => model.Funds)
            </dd>
            <dt class = "col-sm-2">
                @Html.DisplayNameFor(model => model.Registered)
            </dt>
            <dd class = "col-sm-10">
                @Html.DisplayFor(model => model.Registered)
            </dd> 
        </dl>
    
                <div class="form-group">
                    <a>@Html.HiddenFor(model => model.Username)</a>
                    <a>@Html.HiddenFor(model => model.Password)</a>
                    <a>@Html.HiddenFor(model => model.DateCreated)</a>
                    <a>@Html.HiddenFor(model => model.SchoolEmail)</a>
                    <a>@Html.HiddenFor(model => model.CoursesTaken)</a>
                    <a>@Html.HiddenFor(model => model.Concentrations)</a>
                    <a>@Html.HiddenFor(model => model.Funds)</a>
                    <a>@Html.HiddenFor(model => model.Registered)</a>
                    <a>@Html.HiddenFor(model => model.isAdmin)</a>
                <input type="submit" value="Next" class="btn btn-success" />
            </div>
