@model Scheduler.Models.User

<div>
    <form asp-action="ProcessCreate" >
        <h4>Logged In Successfully. User Details</h4>
        <hr />
        <dl class="row">
            <dt class = "col-sm-2">
                @Html.DisplayNameFor(model => model.Username)
            </dt>
            <dd class = "col-sm-10">
                @Html.DisplayFor(model => model.Username)
            </dd>
            <dt class = "col-sm-2">
                @Html.DisplayNameFor(model => model.Password)
            </dt>
            <dd class = "col-sm-10">
                @Html.DisplayFor(model => model.Password)
            </dd>
            <dt class = "col-sm-2">
                @Html.DisplayNameFor(model => model.DateCreated)
            </dt>
            <dd class = "col-sm-10">
                @Html.DisplayFor(model => model.DateCreated)
            </dd>
            <dt class = "col-sm-2">
                @Html.DisplayNameFor(model => model.SchoolEmail)
            </dt>
            <dd class = "col-sm-10">
                @Html.DisplayFor(model => model.SchoolEmail)
            </dd>
            <dt class = "col-sm-2">
                @Html.DisplayNameFor(model => model.CoursesTaken)
            </dt>
            <dd class = "col-sm-10">
                @Html.DisplayFor(model => model.CoursesTaken)
            </dd>
            <dt class = "col-sm-2">
                @Html.DisplayNameFor(model => model.Concentrations)
            </dt>
            <dd class = "col-sm-10">
                @Html.DisplayFor(model => model.Concentrations)
            </dd>
            <dt class = "col-sm-2">
